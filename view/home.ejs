<!doctype HTML>
<html>
<head>
	<title>node-pache</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge;chrome=1;" />
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
	<meta name="HandheldFriendly" content="true" />
	<link rel="stylesheet" href="/style/global.css" type="text/css" />
	<link rel="stylesheet" href="/style/home.css" type="text/css" />
	<link rel="stylesheet" href="/style/normal.css" type="text/css" media="screen and (min-width: 760px)" />
	<link rel="stylesheet" href="/style/min.css" type="text/css" media="screen and (max-width: 759px)" />
</head>
<body>
<ul id="list">
<% for (let item of items){ %>
<% let time = item.time.toLocaleDateString(); %>
	<li>
		<div class="link">
			<a href="/article/<%= item._id %>"><%= item.title %></a>
		</div>
		<div class="datetime"><%= time %></div>
	</li>
<% } %>
</ul>
<div><div id="page"><% include public/pagecode %></div></div>
</body>
</html>
